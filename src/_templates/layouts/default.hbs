<!DOCTYPE html>
<!--[if lt IE 7]>			<html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>				<html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>				<html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->	<html class="no-js"> <!--<![endif]-->
{{> head }}
  <body>
    
    <div class="page-wrap">
      {{> navbar }}
    
      {{> body }}

      {{> footer }}
    
    </div>
    
    {{> aside }}
    
    <script>
      (function(head) {
        head.load([
          "{{link '/assets/scripts/vendor.js' }}",
          "{{link '/assets/scripts/main.js' }}"
          {{#each page.scripts}}
          ,"{{link this}}"
          {{/each}}
        ], function() {
          console.log("loaded scripts");
        });
        head.load([
          {{#each page.stylesheets }}
          "{{ this }}",
          {{/each}}
          "{{!link '/assets/styles/fonts.css'}}"
        ], function() {
          console.log("loaded stylesheets");
        });
      })(head);
    </script>
  </body>
</html>
